            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Pod Restart Count (All Namespaces)",
        "gridPos": {"x": 0, "y": 28, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "kube_pod_container_status_restarts_total{namespace=~\"ricplt|ricxapp\"}",
            "refId": "A",
            "legendFormat": "{{namespace}}/{{pod}}/{{container}}"
          }
        ],
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"]}
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "custom": {
              "axisPlacement": "auto",
              "drawStyle": "line",
              "lineInterpolation": "stepAfter",
              "showPoints": "never",
              "fillOpacity": 0
            }
          }
        }
      },
      {
        "id": 12,
        "type": "stat",
        "title": "Network Receive (ricplt)",
        "gridPos": {"x": 0, "y": 36, "w": 6, "h": 4},
        "targets": [
