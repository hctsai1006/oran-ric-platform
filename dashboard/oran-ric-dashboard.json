          }
        ],
        "options": {
          "tooltip": {"mode": "multi"},
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"]}
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit",
            "custom": {
              "axisPlacement": "auto",
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "showPoints": "never",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 10,
        "type": "timeseries",
        "title": "Memory Usage by xApp (ricxapp)",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(container_memory_working_set_bytes{namespace=\"ricxapp\",pod!=\"\",container!=\"\"}) by (pod)",
