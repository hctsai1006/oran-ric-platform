{
  "dashboard": {
    "title": "QoE Predictor xApp Monitoring",
    "tags": ["oran", "ric", "xapp", "qoe"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "QoE Predictor 狀態",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "up{app=\"qoe-predictor\"}",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]},
          "colorMode": "background"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN", "color": "red"},
              {"type": "value", "value": "1", "text": "RUNNING", "color": "green"}
            ],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "活躍 UE 數",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "qoe_active_ues",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 10, "color": "green"},
                {"value": 50, "color": "yellow"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "預測準確度",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "qoe_prediction_accuracy * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "品質劣化事件",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "qoe_degradation_events_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "預測統計（累計）",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "qoe_predictions_total",
            "legendFormat": "預測總數",
            "refId": "A"
          },
          {
            "expr": "qoe_degradation_events_total",
            "legendFormat": "品質劣化事件",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "次數"},
          {"format": "short"}
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 6,
        "title": "預測速率（每秒）",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(qoe_predictions_total[5m])",
            "legendFormat": "預測速率",
            "refId": "A"
          },
          {
            "expr": "rate(qoe_degradation_events_total[5m])",
            "legendFormat": "劣化事件速率",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "每秒預測數"},
          {"format": "short"}
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "values": true
        }
      },
      {
        "id": 7,
        "title": "預測準確度趨勢",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "qoe_prediction_accuracy * 100",
            "legendFormat": "準確度 (%)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "準確度 (%)", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "UE QoE 分數分佈",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "qoe_score",
            "legendFormat": "UE {{ue_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "QoE 分數", "min": 0, "max": 5},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "預測延遲",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(qoe_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(qoe_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(qoe_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "延遲 (秒)"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "資源使用 - CPU & 記憶體",
        "type": "graph",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{app=\"qoe-predictor\"}[5m]) * 100",
            "legendFormat": "CPU (%)",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{app=\"qoe-predictor\"} / 1024 / 1024",
            "legendFormat": "記憶體 (MB)",
            "refId": "B",
            "yAxisIndex": 1
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU (%)"},
          {"format": "decmbytes", "label": "記憶體 (MB)"}
        ]
      }
    ],
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "version": 1
  },
  "overwrite": true
}
